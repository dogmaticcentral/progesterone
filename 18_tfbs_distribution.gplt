set boxwidth 0.5 relative
set style fill solid 1.0
unset key
set xlabel 'TADs, sequential number'
set ylabel 'number of ESR1 binding sites'
set xrange [0:210]
set yrange [0:0.03]

set terminal png
set output "results/ESR1_and_PGR_tad_distr.png"
plot 'raw_data/ESR1_4_hg19_tad_distribution.tsv' u 1:5 w  boxes, 'raw_data/PGR_4_hg19_tad_distribution.tsv' u ($1+0.7):5 w  boxes
